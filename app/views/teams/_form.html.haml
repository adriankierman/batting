= form_for @team do |f|
  - if @team.errors.any?
    #error_explanation
      %h2= "#{pluralize(@team.errors.count, "error")} prohibited this team from being saved:"
      %ul
        - @team.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :team_city
    = f.text_area :team_city
  .field
    = f.label :team_name
    = f.text_area :team_name
  .field
    = f.label :division
    = f.text_field :division
  .actions
    = f.submit 'Save'
